{
  "name": "xtal-deco",
  "version": "0.0.37",
  "keywords": [
    "Web Components",
    "Custom Elements",
    "Decorator",
    "Built-In"
  ],
  "description": "Add properties / methods to other DOM (custom) elements.",
  "main": "xtal-deco.js",
  "module": "xtal-deco.js",
  "exports": {
    ".": "./xtal-deco.js",
    "./xtal-deco.js": "./xtal-deco.js"
  },
  "files": [
    "xtal-deco.*",
    "types.d.ts"
  ],
  "types": "types.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node node_modules/xtal-test/serve.js",
    "package-check": "package-check",
    "doc": "wca analyze xtal-*.js --outFile custom-elements.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-deco.git"
  },
  "dependencies": {
    "trans-render": "0.0.339"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "nomodule": "0.0.17",
    "on-to-me": "0.0.35",
    "xtal-test": "0.0.51"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-deco/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-deco#readme"
}
